    <uses-permission android:name="android.permission.INTERNET" />
    <!--允许程序访问有关GSM网络信息-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name=".app.MyApplication"

        android:allowBackup="true"
        android:icon="@mipmap/shoping_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZhuBangShopingTest1">
        <activity android:name=".user.man.UserLoginActivity"></activity>
        <activity android:name=".user.man.UserManActivtiy"></activity>
        <activity android:name=".home.ErweimaActivity" />
        <activity android:name=".app.GoodsInfoActivity" />
        <activity android:name=".app.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>